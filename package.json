{
  "name": "stack-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "cljs-once": "shadow-cljs --once",
    "cljs-release": "shadow-cljs --release",
    "cljs": "shadow-cljs --dev",
    "html-dev": "env=dev node entry/render.js",
    "html": "node entry/render.js",
    "hmr": "webpack-dev-server --config webpack.dev.js --hot-only",
    "tiye": "rsync -r dist/ respo.site:repo/Respo/respo.site/",
    "webpack": "webpack --hide-modules --progress",
    "dev": "yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "build": "yarn del && yarn cljs-release && yarn webpack && yarn cljs-once && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "gaze": "^1.1.2",
    "raw-loader": "^0.5.1",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.8.0",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0"
  },
  "shadow-cljs": {
    "source-paths": [
      "src",
      "polyfill"
    ],
    "output-dir": "target/",
    "dependencies": [
      [
        "mvc-works/hsl",
        "0.1.2"
      ],
      [
        "respo/ui",
        "0.1.9"
      ],
      [
        "respo",
        "0.4.3"
      ],
      [
        "respo/markdown",
        "0.1.3"
      ],
      [
        "respo/router",
        "0.2.2"
      ]
    ]
  }
}
