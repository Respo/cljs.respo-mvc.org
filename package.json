{
  "name": "stack-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "cdn": "ls -1 dist | xargs -I % upx put dist/% respo.site/%",
    "tiye": "rsync -r dist/index.html tiye.me:repo/Respo/respo.site/",
    "upload": "yarn cdn && yarn tiye",
    "compile": "shadow-cljs compile app",
    "watch": "shadow-cljs watch app",
    "html-dev": "env=dev node entry/render.js",
    "html": "node entry/render.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist/",
    "build": "yarn compile && yarn del && yarn webpack && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.12.7",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "raw-loader": "^0.5.1",
    "shadow-cljs": "^2.0.1",
    "shortid": "^2.2.8",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "highlight.js": "^9.12.0"
  }
}
