{
  "name": "stack-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/ respo.site:repo/Respo/respo.site/",
    "cljs-once": "shadow-cljs -b app --once",
    "cljs-release": "shadow-cljs -b app --release",
    "cljs": "shadow-cljs -b app --dev",
    "html-dev": "env=dev node entry/render.js",
    "html": "page=all node entry/render.js",
    "hmr": "webpack-dev-server --config webpack.dev.js --hot-only",
    "webpack": "webpack --hide-modules --progress",
    "dev": "yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "build": "yarn del && yarn webpack && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "gaze": "^1.1.2",
    "raw-loader": "^0.5.1",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.9.8",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0"
  }
}
