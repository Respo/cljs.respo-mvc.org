{
  "name": "respo.site",
  "version": "0.1.0",
  "description": "Respo site",
  "main": "index.js",
  "scripts": {
    "cdn": "rsync -avr --progress dist/* tiye.me:cdn/respo.site",
    "tiye": "rsync -avr --progress dist/{index.html,manifest.json} tiye.me:repo/Respo/respo.site/",
    "upload": "yarn cdn && yarn tiye",
    "compile": "shadow-cljs compile app",
    "watch": "shadow-cljs watch app",
    "html-dev": "env=dev node entry/render.js",
    "html": "node entry/render.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn html-dev && yarn dev-manifest && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist/",
    "dev-manifest": "cp entry/manifest.json target/",
    "manifest": "cp entry/manifest.json dist/",
    "build": "yarn compile && yarn del && yarn webpack && yarn html && yarn manifest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/calcit-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffeescript": "^2.1.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.6",
    "http-server": "^0.11.1",
    "raw-loader": "^0.5.1",
    "shadow-cljs": "^2.0.139",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "highlight.js": "^9.12.0"
  }
}
